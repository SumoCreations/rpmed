{
  "name": "rpmed-api",
  "version": "1.0.0",
  "description": "A serverless GraphQL API for the RPMed Customer Service Portal.",
  "main": "app/index.json",
  "scripts": {
    "jestInline": "jest --runInBand --config jestconfig.json",
    "test": "sls offline start --exec \"npm run jestInline\"",
    "testLocal": "npm run dynamo; sls offline start --exec \"npm run jestInline\"",
    "start": "npm run dynamo & serverless offline start -s dev",
    "lint": "tslint -p tsconfig.json",
    "schema": "graphql-codegen --config codegen.yml",
    "startDynamo": "docker run --name dynamoLocal -d -p 8000:8000 amazon/dynamodb-local",
    "stopDynamo": "docker stop dynamoLocal; docker rm dynamoLocal",
    "dynamo": "npm run stopDynamo; npm run startDynamo",
    "prettier": "prettier --config ./.prettierrc 'app/**/*.{ts,tsx}' --write"
  },
  "files": [
    "app/**/*"
  ],
  "dependencies": {
    "apollo-server-lambda": "^3.5.0",
    "bcryptjs": "^2.4.3",
    "googleapis": "^39.2.0",
    "graphql": "^16.2.0",
    "graphql-tools": "^4.0.4",
    "humps": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "luxon": "^1.13.2",
    "middy": "^0.23.0",
    "nodemailer": "^6.4.1",
    "rpmed-schema": "1.0.0",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@graphql-codegen/typescript-react-apollo": "^2.2.3",
    "@graphql-codegen/typescript-resolvers": "^1.19.0",
    "@types/aws-lambda": "^8.10.17",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql": "^14.5.0",
    "@types/humps": "^1.1.2",
    "@types/jest": "^24.0.11",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.123",
    "@types/luxon": "^1.12.0",
    "@types/node": "^10.17.17",
    "@types/nodemailer": "^6.2.2",
    "@types/react": "^16.9.13",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.9.0",
    "@types/yup": "^0.26.23",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "jest": "^24.6.0",
    "jest-raw-loader": "^1.0.1",
    "prettier": "^1.16.4",
    "raw-loader": "^3.1.0",
    "serverless": "^2.69.1",
    "serverless-domain-manager": "^5.2.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^8.3.1",
    "serverless-plugin-graphiql": "^0.4.0",
    "serverless-webpack": "^5.6.0",
    "source-map-support": "^0.5.6",
    "ts-jest": "^24.0.0",
    "ts-loader": "^4.2.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5",
    "webpack": "^4.5.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
