{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es7"
    ],
    "types": [
      "react",
      "jest"
    ],
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src",
    "decs.d.ts"
  ]
}
