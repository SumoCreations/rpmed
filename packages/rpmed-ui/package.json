{
  "name": "rpmed-ui",
  "version": "0.1.0",
  "private": true,
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.4",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.4",
    "@fortawesome/pro-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@hookform/resolvers": "^2.8.2",
    "@sumocreations/forms": "^1.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^9",
    "clsx": "^1.1.1",
    "formik": "2.0.3",
    "immutability-helper": "^3.1.1",
    "postcss": "^7",
    "postcss-cli": "^7",
    "react": "^17.0.2",
    "react-day-picker": "^7.4.10",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "^7.17.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-select": "^5.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "yarn build-tailwind && yarn storybook",
    "build": "rm -rf ./lib && tsc && yarn build-tailwind:release && cp ./src/*.png ./lib",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-tailwind": "postcss src/scss/base.scss -o src/scss/tailwind.scss && postcss src/scss/base.scss -o lib/rpmed-ui.scss",
    "build-tailwind:purged": "postcss src/scss/base.scss -o src/scss/tailwind.scss && ls -lh src/**/*tailwind.scss NODE_ENV=production",
    "build-tailwind:release": "postcss src/scss/base.scss -o lib/rpmed-ui.scss NODE_ENV=production ",
    "build-tailwind:watch": "postcss src/scss/base.scss -o src/scss/tailwind.scss -w",
    "storybook": "start-storybook -p 5000",
    "prettier": "prettier --config ../../.prettierrc 'src/**/*.{js,jsx,ts,tsx}' --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-controls": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@types/react-router-dom": "^5.3.1",
    "babel-loader": "8.1.0",
    "css-loader": "^4.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^11.0.1",
    "storybook-addon-designs": "^6.2.0",
    "style-loader": "^2.0.0"
  }
}
