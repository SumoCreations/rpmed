{
  "name": "rpmed-ui",
  "version": "1.0.0",
  "private": true,
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.4",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.4",
    "@fortawesome/pro-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@hookform/resolvers": "^2.8.2",
    "@sumocreations/forms": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^10.4.0",
    "clsx": "^1.1.1",
    "formik": "^2.2.9",
    "immutability-helper": "^3.1.1",
    "postcss-loader": "^6.2.1",
    "react": "^17.0.2",
    "react-day-picker": "^7.4.10",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "^7.17.5",
    "react-router": "^6",
    "react-router-dom": "^6",
    "react-select": "^5.1.0",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.1.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "yarn storybook",
    "build": "NODE_ENV=production rm -rf ./lib && tsc && yarn build-tailwind && cp ./src/*.png ./lib",
    "build-tailwind": "postcss ./src/css/base.css -o ./src/css/tailwind.css && postcss ./src/css/base.css -o ./lib/rpmed-ui.css",
    "build-tailwind:watch": "npx tailwindcss -i ./src/css/base.css -o ./src/css/tailwind.css  --watch & npx tailwindcss -i ./src/css/base.css -o ./lib/rpmed-ui.css  --watch",
    "storybook": "start-storybook -p 6006",
    "prettier": "prettier --config ../../.prettierrc 'src/**/*.{js,jsx,ts,tsx}' --write",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-controls": "^6.4.9",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "babel-loader": "8.1.0",
    "css-loader": "^4.2.2",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "sass": "^1.45.1",
    "sass-loader": "^11.0.1",
    "storybook-addon-designs": "^6.2.0",
    "style-loader": "^2.0.0"
  }
}
